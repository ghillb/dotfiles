{
  "Class": {
    "prefix": ["num2ord"],
    "body": [
      "fn num_to_ordinal_expr(x: u32) -> String {",
          "\tformat!(\"{}{}\", x, match (x % 10, x % 100) {",
              "\t\t(_, 4..=20) => \"th\",",
              "\t\t(1, _) => \"st\",",
              "\t\t(2, _) => \"nd\",",
              "\t\t(3, _) => \"rd\",",
              "\t\t_ => \"th\",",
          "\t})",
      "}"
    ],
    "description": "Function that converts numbers to ordinals"
  }
}

